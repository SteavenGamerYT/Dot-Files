# Rx 7600 XT
output "HDMI-A-2" {
    mode  1366x768@59.79Hz
    pos 0 0
    transform normal
    scale 1.0
    scale_filter nearest
    adaptive_sync off
    dpms on
}
output "HDMI-A-1" {
    mode  1920x1080@60.0Hz
    pos 0 768
    transform normal
    scale 1.0
    scale_filter nearest
    adaptive_sync off
    dpms on
}
workspace 1 output HDMI-A-2
workspace 2 output HDMI-A-1

# GTX 1650
output "DVI-D-1" disable
output "HDMI-A-3" disable
output "DP-3" disable
