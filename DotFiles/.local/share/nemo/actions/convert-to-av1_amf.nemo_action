[Nemo Action]
Name=Convert to AV1 (AMF)
Comment=Convert the selected video to AV1 format with AAC audio
Quote=double
Exec=kitty sh -c 'input=%F; output="${input%.*}-av1_amf.${input##*.}"; ffmpeg -i "$input" -c:v av1_amf -c:a aac "$output"'
Icon-Name=media-video
Selection=File
Extensions=avi;mp4;mkv;mov;webm;flv;wmv;mpeg;